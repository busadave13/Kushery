@{
    ViewData["Title"] = "Lake Forest Park";
}

@section head {
    <link rel="Stylesheet" href="@Href("~/css/locationpage.css")" />
}
<div class="container-fluid">

    @*banner*@

    <section class="location-banner-lakeforestpark">
        <label class="location-banner-name location-banner-coustom">Lake Forest Park</label>
    </section>


    @*column one*@

    <div class="col-sm-8">
        <div class="bio">

            <p>
                Located just East of I-5 between exits 177 & 178 in the old Guitarville building at 15th Ave. N.E. & N.E. 196th.
                Stop in and meet your local neighborhood budtenders, who are always happy to see you and ready to help you pick out some quality products.  At the Kushery you’ll find that we have a great selection at competitive prices, you won’t leave disappointed.
                In addition we at the Kushery are 100% pet friendly, so feel free to bring in your fur buddies to say hello to ours.
            </p>

        </div>




        @*map*@

        <div class="map">




            <iframe frameborder="0" style="border: 0" src="https://www.google.com/maps/embed/v1/place?q=19258 15th Ave NE Lake Forest park WA 98155&amp;key=AIzaSyDK08tC4NRubbIiw-xwDR1WEp-YAXX1Mx8" allowfullscreen=""></iframe>

            <span class="all">
                <span class="specials_monday_tuesday">

                    <ul>
                        <li>
                            <h4>Monday: Make your Monday "Golden"</h4>
                            <p>Golden Tree Flower</p>
                            <p>1g &#64 $10</p>
                            <p>3.5g &#64 $35</p>
                            <p>14g &#64 $120</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Golden Tree Joints</h4>
                            <p>1/2g &#64 $4</p>
                            <p>1g &#64 $7</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Tuesday/Top Shelf:</h4>
                            <p>Flower- </p>
                            <p>1g &#64 $12</p>
                            <p>2g &#64 $20</p>
                            <p>3.5g &#64 $35</p>
                            <p>7g &#64 $65</p>
                            <p>14g &#64 $120</p>
                            <p>28g &#64 $230</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Joints-</h4>
                            <p>1g &#64 $6</p>
                            <p>2pk 1/2g &#64 $7</p>
                        </li>
                    </ul>


                    <ul>
                        <li>
                            <h4>Carts</h4>
                            <p>&#64 $28</p>
                        </li>
                    </ul>

                </span>



                <span class="specials_wednesday_thursday">
                    <ul>
                        <li>
                            <h4>Wax Wednesday:</h4>
                            <p>Viva Derado &#64 $25</p>
                            <p>Viva Fire &#64 $18</p>
                            <p>S.P.P. &#64 $28</p>
                            <p>S.P.P. Regulator &#64 $22</p>
                            <p>Doc Croc &#64 $28</p>
                            <p>Sticky Buds &#64 $25</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Thursday/High End Grams:</h4>
                            <p>$14-$15g &#64 $12.00</p>
                            <p>$50 1/8’s &#64 $45.00</p>
                            <p>$45 1/8’s &#64 $40.00</p>
                        </li>
                    </ul>
                </span>

                <span class="specials_friday_saturday_sunday">

                    <ul>

                        <li>
                            <h4>Friday:</h4>
                            <p>3 items 5% off</p>
                            <p>5 items 10% off</p>
                            <p>7 items 15% off</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Saturday:</h4>
                            <p>Top Shelf &#64 $30</p>
                            <p>Avitas &#64 $30</p>
                            <p>RSO Go 1/2g &#64 $22</p>
                            <p>RSO Go 1g &#64 $40</p>
                        </li>
                    </ul>

                    <ul>
                        <li>
                            <h4>Sunday:</h4>
                            <p>*Customers choice*</p>
                        </li>
                    </ul>


                </span>
            </span>
        </div>
    </div>
    




    @*address*@

    <div class="col-sm-4">
        <div class="address">
            <p><b>The Kushery Lake Forest Park:</b></p>
            <p>19258 15th Ave. NE STE B</p>
            <p>Lake Forest Park, WA. 98155</p>
            <p><b>Phone:</b> 206-403-1106</p>
        </div>


        @*social media*@


        <div class="socail_media_pictures">
            <div class="social-media">
                <a href="https://www.facebook.com/TheKusheryLakeForestPark/ ">
                    <img title="facebook" alt="facebook" src="~/images/facebookLogo.PNG" />
                </a>
                <a href="https://www.instagram.com/lfpkushery/?hl=en ">
                    <img title="instagram" alt="instagram" src="~/images/instagram.png" />
                </a>
                <p><h4>Follow us on social media!</h4></p>
            </div>
        </div>

        @*hours*@

        <section class="information-hours">
            <h2>Hours</h2>
            <table>
                <tbody>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Sunday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Monday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>
                        @*<td class="spacer-vertical-xxs spacer-left-xs">style="color: green"</td>*@
                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Tuesday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Wednesday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Thursday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Friday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 10:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Saturday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 10:00 PM</td>

                    </tr>
                </tbody>
            </table>
        </section>


        @*employee carousel*@

        <div class="employee-carousel" style=" width:450px;  margin:0 auto;">
            <div class="carousel heading">
                <h3>Our Lake Forest Park Employees:</h3>
            </div>
            <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>
                </ol>
                <div class="carousel-inner" role="listbox">
                    <div class="item active">
                        <img src="~/images/LakeForestemp1.JPG" alt="ASP.NET" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                            <p><h3>Cassie</h3></p>
                        </div>
                    </div>
                    <div class="item">
                        <img src="~/images/Gunner.jpg" alt="Package Management" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                            <p><h3>Gunner</h3></p>
                        </div>
                    </div>
                    <div class="item">
                        <img src="~/images/LakeForestemp2.jpg" alt="Visual Studio" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                            <p><h3>Ryan</h3></p>
                        </div>
                    </div>
                    <div class="item">
                        <img src="~/images/LakeForestemp3.jpg" alt="Package Management" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                            <p><h3>Angel</h3></p>
                        </div>
                    </div>

                    @*<div class="item">
                                <img src="~/images/emp4.jpg" alt="Microsoft Azure" class="img-responsive" />
                                <div class="carousel-caption" role="option">
                                </div>
                            </div>
                        </div>*@

                    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>



                </div>


            </div>

        </div>

    </div>
</div>























