@{
    ViewData["Title"] = "Cathcart";
}

@section head {
    <link rel="Stylesheet" href="@Href("~/css/locationPage.css")" />
}


@*banner*@

<section class="location-banner">
   
    <label class=" location-banner-name">Cathcart</label>
</section>


<div class="row">


    <div class="col-sm-3">

        <div class="specials">
            <p><b>Specials</b></p>
           <p>10% off for Veterans, Medical Patients and 
            when you come in on your birthday.</p>
            <p>Can <b>not</b> Combine Discounts.</p>
            
            <p>Vendor Day- Thursdays and Fridays.</p> 

        </div>
    
 </div>



    <div class="col-sm-6">
        <div class="bio">
            <p>
                <p>Conveniently located West of Highway 9 & East of I-5 in the Silver Firs Station area on 134th Pl S.E.</p>
                <p>At the Kushery in Everett you’ll be pleased to be greeted by a friendly budtender who is eager to meet all your marijuana needs.  We have an amazing selection of flower, concentrates, edibles, and so much more at competitive prices.  So, stop in and let our helpful staff set you up with some quality cannabis.</p>

                <p>You can’t miss us located in the Whistle Stop strip mall, where you can stock up on smoke and put any munchie cravings to rest all at once.</p>
           
        </div>

        <div class="map">
            <iframe frameborder="0" style="border: 0" src="https://www.google.com/maps/embed/v1/place?q=18026 state route 9, Everett WA 98208 &amp;key=AIzaSyDK08tC4NRubbIiw-xwDR1WEp-YAXX1Mx8" allowfullscreen=""></iframe>
        </div>
    </div>

    <div class="col-sm-3">
        <div class="address">
            <p><b>The Kushery Everett:</b></p>
            <p>18026 State Route 9, STE B</p>
            <p>Everett, WA. 98208</p>
            <p><b>Phone:</b> 425-337-5145</p>
        </div>

        <section class="information-hours">
            <h2>Hours</h2>
            <table>
                <tbody>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Sunday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Monday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                        <td class="spacer-vertical-xxs spacer-left-xs" style="color: green">Open Now</td>
                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Tuesday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Wednesday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Thursday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Friday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                    <tr>
                        <td class="spacer-vertical-xxs spacer-right-xs"><b>Saturday</b></td>
                        <td class="spacer-vertical-xxs spacer-horizontal-xs">10:00 AM - 8:00 PM</td>

                    </tr>
                </tbody>
            </table>
        </section>


        <div class="employee-carousel">
            <h3>Our Cathcart Employees.</h3>
            <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    @*<li data-target="#myCarousel" data-slide-to="2"></li>
                    <li data-target="#myCarousel" data-slide-to="3"></li>*@
                </ol>
                <div class="carousel-inner" role="listbox">
                    <div class="item active">
                        <img src="~/images/Everettemp1.JPG" alt="ASP.NET" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                        </div>
                    </div>
                    <div class="item">
                        <img src="~/images/Everettemp2.JPG" alt="Visual Studio" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                        </div>
                    </div>
                    @*<div class="item">
                        <img src="~/images/emp3.jpg" alt="Package Management" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                        </div>
                    </div>
                    <div class="item">
                        <img src="~/images/emp4.jpg" alt="Microsoft Azure" class="img-responsive" />
                        <div class="carousel-caption" role="option">
                        </div>
                    </div>
                </div>*@

                <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>

  </div>
</div>























